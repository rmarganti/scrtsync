{
    "$schema": "https://raw.githubusercontent.com/rmarganti/scrtsync/main/schemas/scrtsync.schema.1.0.0.json",
    "presets": {
        "pull": {
            "from": "vault://secret/path/to/your/secrets",
            "to": "file://.env"
        },

        "push": {
            "from": "file://.env",
            "to": "vault://secret/path/to/your/secrets"
        }
    }
}
